<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.views.HomeTransporteurAssocieController">
    <children>
        <AnchorPane prefHeight="50.0" prefWidth="1000.0" style="-fx-background-color: #582900;">
         <children>
            <Label fx:id="labelprofile" layoutX="58.0" onMouseClicked="#goToProfile" prefHeight="50.0" prefWidth="189.0" text="Name du User" textFill="#fffdfd">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Label>
            <Button layoutX="911.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#logOut" prefHeight="27.0" prefWidth="80.0" text="Logout">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
        <AnchorPane layoutY="50.0" prefHeight="750.0" prefWidth="200.0" style="-fx-background-color: #C8AD7F;">
         <children>
            <Button layoutX="17.0" layoutY="50.0" mnemonicParsing="false" onAction="#goToLivraison" prefHeight="50.0" prefWidth="167.0" style="-fx-background-color: #582900;" text="Vos Livraisons" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
            <Button layoutX="17.0" layoutY="146.0" mnemonicParsing="false" onAction="#goToLivraisionEnCours" prefHeight="50.0" prefWidth="167.0" style="-fx-background-color: #582900;" text="Livraison en cours" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
            <Button layoutX="17.0" layoutY="231.0" mnemonicParsing="false" onAction="#goToContrat" prefHeight="50.0" prefWidth="167.0" style="-fx-background-color: #582900;" text="Contrat" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children></AnchorPane>
      <AnchorPane fx:id="anchorLoad" layoutX="200.0" layoutY="50.0" prefHeight="750.0" prefWidth="800.0" />
    </children>
</AnchorPane>
